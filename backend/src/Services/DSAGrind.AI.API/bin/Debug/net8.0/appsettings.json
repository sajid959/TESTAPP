{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AI": {
    "Host": "0.0.0.0",
    "Port": "5003"
  },
  "Auth": {
    "Authority": "http://localhost:8080"
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:5000", "https://localhost:5000"]
  },
  "JwtSettings": {
    "Audience": "DSAGrind-Users"
  },
  "AISettings": {
    "Provider": "Perplexity",
    "ApiKey": "${PERPLEXITY_API_KEY}",
    "BaseUrl": "https://api.perplexity.ai"
  }
}
