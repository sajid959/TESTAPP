{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Search": {
    "Host": "0.0.0.0",
    "Port": "5004"
  },
  "Auth": {
    "Authority": "http://localhost:8080"
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:5000", "https://localhost:5000"]
  },
  "JwtSettings": {
    "Audience": "DSAGrind-Users"
  },
  "MongoDbSettings": {
    "ConnectionString": "${MONGODB_CONNECTION_STRING:mongodb://localhost:27017}",
    "DatabaseName": "${MONGODB_DATABASE_NAME:dsagrind}"
  },
  "QdrantSettings": {
    "Url": "${QDRANT_URL:http://localhost:6333}",
    "ApiKey": "${QDRANT_API_KEY:}",
    "GrpcPort": ${QDRANT_GRPC_PORT:6334}
  }
}
